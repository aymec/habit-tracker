{
  "expo": {
    "name": "OnTrack",
    "slug": "ontrack",
    "version": "1.0",
    "orientation": "portrait",
    "icon": "./assets/images/Tracks_Light/apple-devices/AppIcon.appiconset/icon-ios-1024x1024.png",
    "scheme": "ontrack",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aym3r1c.ontrack",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/images/Tracks_Light/android/Tracks_Light_Icon.png",
        "backgroundImage": "./assets/images/Tracks_Light/android/Tracks_Light_Background.png",
        "monochromeImage": "./assets/images/Tracks_Light/android/Tracks_Light_Monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.aym3r1c.ontrack"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Tracks_Light/apple-devices/AppIcon.appiconset/icon-ios-1024x1024.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000",
            "image": "./assets/images/Tracks_Dark/apple-devices/AppIcon.appiconset/icon-ios-1024x1024.png"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "132afac7-356a-4b38-9986-ae6af20dd24b"
      }
    }
  }
}
