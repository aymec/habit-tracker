{
  "expo": {
    "name": "OnTrack",
    "slug": "ontrack",
    "version": "1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-light-1024x1024.png",
    "scheme": "ontrack",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aym3r1c.ontrack",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/images/icon-light-android-foreground.png",
        "backgroundImage": "./assets/images/icon-light-android-background.png",
        "monochromeImage": "./assets/images/icon-light-android-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.aym3r1c.ontrack"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon-light-1024x1024.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000",
            "image": "./assets/images/icon-dark-1024x1024.png"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true,
      "baseUrl": ""
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "132afac7-356a-4b38-9986-ae6af20dd24b"
      }
    }
  }
}
